{
  "name": "typebox-superforms",
  "version": "0.1.0",
  "private": true,
  "description": "Adapters for using TypeBox with SvelteKit Superforms",
  "author": "Joseph T. Lapp <arachnojoe@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jtlapp/typebox-superforms"
  },
  "keywords": [
    "typebox",
    "validation",
    "forms",
    "sveltekit",
    "superforms",
    "adapter"
  ],
  "license": "MIT",
  "type": "module",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rm -rdf dist && tsc",
    "clean": "rm -rf node_modules && rm -rf dist",
    "dev": "tsc -w",
    "lint": "TIMING=1 eslint \"src/**/*.ts*\"",
    "test": "# vitest --run"
  },
  "devDependencies": {
    "@types/node": "^20.4.6",
    "eslint": "^8.40.0",
    "eslint-config-custom": "workspace:*",
    "sveltekit-superforms": "^1.5.0",
    "tsconfig": "workspace:*",
    "typescript": "5.1.3",
    "vitest": "^0.32.2",
    "vitest-config": "workspace:*"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.30.2",
    "typebox-validators": "0.3.1"
  }
}
