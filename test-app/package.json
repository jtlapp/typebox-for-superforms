    "eslint": "^8.40.0",
    "eslint-config-custom-svelte": "workspace:*",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "sveltekit-superforms": "^1.5.0",
    "tsconfig": "workspace:*",
    "tslib": "^2.4.1",
    "typebox-superforms": "workspace:*",
    "typescript": "5.1.3",
    "vite": "^4.4.2",
    "vite-tsconfig-paths": "^4.2.0",
    "zod": "^3.21.4"
